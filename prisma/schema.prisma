generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id Int @id @default(autoincrement())
}

model shoutbox {
  id        Int       @id @default(autoincrement())
  message   String?   @db.VarChar(512)
  author    String?   @db.VarChar(32)
  timestamp DateTime? @db.Date
}
